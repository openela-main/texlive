%postun t2
if [ $1 == 0 ] ; then
sed -i 's/^cyramstex.*$/\#\!\ cyramstex pdftex language.dat -translate-file=cp227.tcx *cyramstx.ini/' %{_texdir}/texmf-dist/web2c/fmtutil.cnf > /dev/null 2>&1
