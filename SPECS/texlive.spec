# luatex
%ifnarch aarch64 %{mips} %{power64} s390 s390x
pushd %{buildroot}%{_bindir}
for f in texluajit texluajitc ; do
 ln -fs luajittex $f
done
popd
%endif

# fix context
pushd %{buildroot}%{_bindir}
rm -f context
cat > context << EOF
